#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vancc369/proyectos/cotizador/bcotizador/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.9.3_pg@8.11.3_ts-node@10.9.2_@types+node@20.11.30_typescript@5.4.2_/node_modules/typeorm/node_modules:/home/vancc369/proyectos/cotizador/bcotizador/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.9.3_pg@8.11.3_ts-node@10.9.2_@types+node@20.11.30_typescript@5.4.2_/node_modules:/home/vancc369/proyectos/cotizador/bcotizador/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vancc369/proyectos/cotizador/bcotizador/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.9.3_pg@8.11.3_ts-node@10.9.2_@types+node@20.11.30_typescript@5.4.2_/node_modules/typeorm/node_modules:/home/vancc369/proyectos/cotizador/bcotizador/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.9.3_pg@8.11.3_ts-node@10.9.2_@types+node@20.11.30_typescript@5.4.2_/node_modules:/home/vancc369/proyectos/cotizador/bcotizador/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../cli-ts-node-esm.js" "$@"
fi
